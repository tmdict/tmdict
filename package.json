{
  "name": "tmdict",
  "private": true,
  "version": "4.0.0",
  "type": "module",
  "scripts": {
    "build": "npm run clean && npm run compile && vite build",
    "clean": "rimraf bin build src/lib/__generated static/__generated static/legacy",
    "clean:deep": "npm run clean && rimraf node_modules .svelte-kit",
    "compile": "tsc -p ./scripts && node bin/main.js",
    "dev": "npm run compile && vite dev",
    "prepare": "svelte-kit sync || echo ''",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/enhanced-img": "^0.9.3",
    "@sveltejs/kit": "^2.50.1",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@types/fs-extra": "^11.0.4",
    "@types/postcss-import": "^14.0.3",
    "@types/uglify-js": "^3.17.5",
    "cssnano": "^7.1.2",
    "fs-extra": "^11.3.3",
    "fuse.js": "^7.1.0",
    "gray-matter": "^4.0.3",
    "handlebars": "^4.7.8",
    "marked": "^17.0.1",
    "postcss": "^8.5.3",
    "postcss-import": "^16.1.0",
    "rimraf": "^6.1.2",
    "sharp": "^0.34.5",
    "sitemap": "^9.0.0",
    "svelte": "^5.48.0",
    "typescript": "^5.9.3",
    "uglify-js": "^3.19.3",
    "vite": "^7.3.1"
  }
}
